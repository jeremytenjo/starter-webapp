{
  "name": "app",
  "description": "App starter",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node --experimental-json-modules --loader ts-node/esm ./scripts/dev/run.ts",
    "app:dev": "node --loader ts-node/esm ./devtools/vite/scripts/startDevServer/run.ts",
    "app:build": "node --loader ts-node/esm ./devtools/vite/scripts/buildApp/run.ts",
    "app:serve": "node --loader ts-node/esm ./devtools/vite/scripts/runBuild/run.ts",
    "storybook:dev": "start-storybook -c ./devtools/storybook -p 6007 --quiet --ci",
    "emulators:start": "firebase emulators:start --only auth,firestore,functions",
    "functions:dev": "concurrently 'npm run functions:build-watch' 'npm run emulators:start'",
    "functions:build": "node ./functions/scripts/buildFunctions/run.js",
    "functions:build-watch": "node ./functions/scripts/buildFunctions/run.js --watch",
    "functions:deploy": "node ./functions/scripts/deployFunctions/run.js",
    "helpers:generate-logo-assets": "node --loader ts-node/esm ./devtools/helpers/assets/generateLogoAssets/run.ts",
    "helpers:update-useweb-deps": "npm-check-updates -f /@useweb/*/ -u && npm i",
    "testing:run": "./scripts/tests/run",
    "testing:cypress:run": "cypress run",
    "testing:cypress:open": "cypress open",
    "testing:lint": "eslint ./ --fix && tsc --noemit",
    "prettify": "node --loader  ts-node/esm ./devtools/prettier/scripts/runPrettier/runPrettier.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.2.7",
    "@storybook/addon-essentials": "^6.4.9",
    "@useweb/array-db": "0.3.0",
    "@useweb/capitalize": "0.1.0",
    "@useweb/remove-last-character": "0.1.0",
    "@useweb/use-async": "^1.1.0",
    "@useweb/use-data": "^5.1.2",
    "@useweb/use-event-listener": "^0.2.0",
    "@useweb/use-firebase": "^7.1.8",
    "@useweb/use-firebase-function": "^2.3.0",
    "@useweb/use-install-prompt": "^1.1.0",
    "@useweb/use-local-storage": "^1.2.1",
    "@useweb/use-pull-to-refresh": "^2.0.0",
    "enquirer": "^2.3.6",
    "firebase": "^9.6.2",
    "firebase-admin": "^10.0.1",
    "history": "^5.2.0",
    "motion": "^10.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.22.5",
    "react-router-dom": "^6.2.1",
    "swr": "^1.1.2",
    "zustand": "^3.6.8"
  },
  "devDependencies": {
    "@check-light-or-dark/color": "^1.0.3",
    "@grpc/grpc-js": "^1.5.0",
    "@storybook/addon-controls": "^6.4.9",
    "@storybook/addon-docs": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@types/estree": "0.0.50",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@vitejs/plugin-react": "^1.1.4",
    "chalk": "^5.0.0",
    "command-line-args": "^5.2.0",
    "concurrently": "^7.0.0",
    "css-loader": "^6.5.1",
    "cypress": "^9.2.1",
    "dotenv": "^10.0.0",
    "dotenv-parse-variables": "^2.0.0",
    "eslint": "^8.6.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.5",
    "filehound": "^1.17.5",
    "format-duration": "^1.4.0",
    "husky": "^7.0.4",
    "ink": "^3.2.0",
    "internal-ip": "^7.0.0",
    "live-server": "^1.2.1",
    "lodash": "^4.17.21",
    "node-emoji": "^1.11.0",
    "npm-check-updates": "^12.1.0",
    "prettier": "^2.5.1",
    "qrcode-terminal": "^0.12.0",
    "react-json-view": "^1.21.3",
    "rollup-plugin-manifest-json": "^1.5.1",
    "rollup-plugin-visualizer": "^5.5.2",
    "sharp": "^0.29.3",
    "storybook": "^6.4.9",
    "style-loader": "^3.3.1",
    "tcp-port-used": "^1.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "vite": "^2.7.10",
    "vite-plugin-compression": "^0.3.6",
    "vite-plugin-html": "^2.1.2",
    "vite-plugin-pwa": "^0.11.12"
  }
}
