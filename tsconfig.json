{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": true,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "*": ["devtools/ts/declaredModules/*"],
      "virtual:pwa-register": ["node_modules/vite-plugin-pwa"]
    },
    "types": ["cypress", "vite/client", "vite-plugin-pwa/client"]
  },
  "include": ["./src", "./functions/scripts", "devtools/ts/declaredModules/*"],
  "files": ["node_modules/vite-plugin-pwa/client.d.ts"]
}
