{
  "name": "webapp",
  "version": "1.0.0",
  "description": "webapp",
  "main": "index.js",
  "author": "je",
  "license": "ISC",
  "scripts": {
    "start": "yarn webpack-scripts start",
    "build": "yarn webpack-scripts build",
    "build-analyze": "yarn webpack-scripts build && yarn webpack-scripts analyze",
    "deploy": "yarn lint && yarn pretty && yarn webpack-scripts build && gpush && firebase deploy",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "pretty": "prettier \"**/*.js\" --write",
    "check-staged": "lint-staged",
    "figma-generate": "figma-theme JDAMhYaQ9pStIBndyZUCcY4A  --out-dir src/Theme/Theme.Figma",
    "icons-build": "svg2react-icon src/Images/icons src/Misc/Icons/Icons.Set",
    "lint": "eslint ./src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": ["eslint --fix-dry-run", "prettier --list-different", "git add"]
  },
  "browserslist": ["> 0.5%", "last 2 versions", "not dead"],
  "devDependencies": {
    "eslint": "^5.14.1",
    "storybook-addon-prettier-source": "^2.1.2"
  },
  "dependencies": {
    "@material-ui/core": "^3.8.1",
    "@storybook/addon-options": "^4.1.12",
    "@storybook/addon-viewport": "^4.1.12",
    "@storybook/react": "^5.0.0-rc.1",
    "@tenjo/eslint-config": "^0.7.0",
    "@tenjo/webpack-config": "^3.7.0",
    "babel-eslint": "^10.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "figma-js": "^1.6.0",
    "figma-theme": "^1.0.0-3",
    "firebase": "^5.8.2",
    "firebase-admin": "^7.0.0",
    "firebase-functions": "^2.1.0",
    "firebase-tools": "^6.2.2",
    "get-my-local-ip": "^0.1.16",
    "htmltidy": "^0.0.6",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4",
    "react": "^16.8.1",
    "react-devtools": "^3.4.3",
    "react-dom": "^16.8.1",
    "react-router-dom": "^4.2.2",
    "reactn": "^1.0.0",
    "storybook": "^1.0.0",
    "storybook-addon-figma": "^0.1.0",
    "storybook-addon-jsx": "^6.0.0",
    "styled-components": "^4.1.3",
    "svg2react-icon": "^3.0.10"
  }
}
